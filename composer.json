{
  "name" : "lcm/brightcove-bundle",
  "description" : "Symfony LCMBrightcoveBundle",
  "type" : "symfony-bundle",
  "authors" : [{
    "name" : "Jacob Spizziri",
    "email" : "jacob.spizziri@gmail.com"
  }],
  "keywords" : [
    "brightcove"
  ],
  "license" : [
    "MIT"
  ],
  "require" : {
    "brightcove/api": "@dev"
  },
  "autoload" : {
    "psr-0" : {
      "LCM\\BrightcoveBundle" : ""
    }
  },
  "target-dir" : "LCM/BrightcoveBundle",
  "repositories" : [
    {
      "type" : "package",
      "package" : {
        "name" : "brightcove/api",
        "version" : "1.0",
        "source" : {
          "url" : "https://github.com/brightcove/PHP-API-Wrapper.git",
          "type" : "git"
        },
        "autoload" : {
          "classmap" : ["*.php"]
        }
      }
    }
  ],
  "extra" : {
    "branch-alias" : {
      "dev-master" : "1.0.x-dev"
    }
  }
}